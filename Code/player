const sword = {
  XPosLastFrame: 0,
  YPosLastFrame: 0,
  updateX: () => XPosLastFrame = mouseX,
  updateY: () => YPosLastFrame = mouseY,
  getSpeed: () => dist(mouseX, mouseY, sword.XPosLastFrame, sword.YPosLastFrame)
};
